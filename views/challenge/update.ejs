<h2>Challenge Edit</h2>

<form method="POST">
<table class="table" style="width: 300px">
  <tr>
    <td>Name:</td>
    <td><input type="text" name="Challenge[name]" value="<%= challenge.name %>" /></td>
  </tr>
  <tr>
    <td>Description:</td>
    <td><input type="text" name="Challenge[description]" value="<%= challenge.description %>" /></td>
  </tr>
  <tr>
    <td>Start Date:</td>
    <td><input type="text" name="Challenge[startDate]" value="<%= challenge.startDate %>" /></td>
  </tr>
  <tr>
    <td><input type="submit" class="btn btn-default"></td>
  </tr>
</table>
</form>

